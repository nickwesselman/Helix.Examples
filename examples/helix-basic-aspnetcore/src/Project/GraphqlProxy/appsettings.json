{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
      "EndPoints": {
          "Http": {
              "Url": "http://localhost:5151"
          }
      }
  },
  "ReverseProxy": {
    "Routes": {
      "edge": {
          "ClusterId": "edge",
          "Match": {
            "Path": "{**catch-all}"
          },
          "Transforms": [
            {
              "RequestHeader": "sc_apikey",
              "Set": ""
            }
          ]
      }
    },
    "Clusters": {
      "edge": {
        "Destinations": {
          "edge": {
            "Address": "https://edge-beta.sitecorecloud.io/"
          }
        }
      }
    }
  }
}
